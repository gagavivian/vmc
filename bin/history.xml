<application>
	<app-name>history</app-name>
	<app-type>Java</app-type>
	<version>1.6</version>
	<isService>true</isService>
	<cService>
		<arg name="key" />		
	</cService>
	<requirements>		
		<requirement type="port" name="port" index="0">
			<destinations>
				<destination type="cmd"/>						
			</destinations>					
		</requirement>
		<requirement type="port" name="port2" index="1">
			<destinations>
				<destination type="xml" path="/settings.xml" xpath="*/port"/>
			</destinations>					
		</requirement>		
	</requirements>
</application>
